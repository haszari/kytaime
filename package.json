{
  "name": "kytaime",
  "productName": "Kytaime Patterns Reality",
  "description": "browser MIDI pattern sequencer prototype",
  "version": "0.0.1",
  "private": true,
  "main": "./src/electron-main.js",
  "scripts": {
    "build": "webpack",
    "clean": "rm -rf dist/",
    "make-mac-icns": "iconutil -c icns -o src/assets/icons/kytaime-logo.icns src/assets/kytaime.iconset",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=packages --icon=src/assets/icons/kytaime-logo.icns",
    "start-production": "export NODE_ENV=production; node dist/server/server.js",
    "start-server": "export NODE_ENV=development; node src/server/server.js",
    "start": "export NODE_ENV=development; node_modules/.bin/electron ."
  },
  "dependencies": {
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.26.1",
    "express": "^4.16.3",
    "foundation-sites": "^6.3.0",
    "html-webpack-plugin": "^2.22.0",
    "lodash": "^4.17.10",
    "midievents": "^1.0.1",
    "midifile": "^1.0.4",
    "node-sass": "^4.9.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.9",
    "redux": "^3.6.0",
    "sass-loader": "^4.1.0",
    "style-loader": "^0.13.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.6.0",
    "copy-webpack-plugin": "^4.5.2",
    "electron": "^1.8.7",
    "file-saver": "^1.3.8",
    "hjson": "^2.4.1",
    "html-loader": "^0.4.3",
    "immutability-helper": "^2.7.1",
    "moment": "^2.22.2",
    "script-loader": "^0.7.0",
    "shortid": "^2.2.6",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.5.1",
    "worker-loader": "^0.7.1"
  }
}
